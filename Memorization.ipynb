   "source": [
    "from functools import lru_cache\n",
    "\n",
    "@lru_cache(maxsize=None) # Unbounded cache\n",
    "def fib(n):\n",
    "    if n < 2:\n",
    "        return n\n",
    "    return fib(n-1) + fib(n-2)\n",
    "\n",
    "print(fib(1000)) "
   ]
  },
